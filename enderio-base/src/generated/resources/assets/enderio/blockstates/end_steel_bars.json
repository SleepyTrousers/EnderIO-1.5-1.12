{
  "multipart": [
    {
      "apply": {
        "model": "enderio:block/end_steel_bars_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_no_side"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_no_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_no_side",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "enderio:block/end_steel_bars_no_side_alt"
      }
    }
  ]
}